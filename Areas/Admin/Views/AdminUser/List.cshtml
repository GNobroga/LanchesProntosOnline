@model IEnumerable<IdentityUser>
  @using Microsoft.AspNetCore.Identity

  <div class="container mt-4 d-flex flex-column gap-3">
    <h1>Membros</h1>

    @if (Model.Count() == 0) {
    <p class="text-center">Não há nenhuma membro cadastro</p>
    } else {
        <table class="table">
        <thead>
            <tr>
            <th>Código</th>
            <th>Usuário</th>
            <th>E-mail</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model) {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                </tr>
            }
            
        </tbody>
        </table>
    }
  </div></IdentityUser
>
